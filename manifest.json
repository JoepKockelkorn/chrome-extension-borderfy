{
  "name": "Borderfy Elements",
  "description": "Toggle the extension to visualize each HTML element with colors and borders",
  "manifest_version": 2,
  "version": "1.0",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Print this page",
    "default_icon": "print_16x16.png"
  },
  "permissions": ["activeTab"]
}
